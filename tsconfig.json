{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      "@modules/*": ["./src/modules/*"],
      "@assets/*": ["./src/assets/*"],
      "@uiCore/*": ["./src/uiCore/*"],
      "@contains/*": ["./src/contains/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
